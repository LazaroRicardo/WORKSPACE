 "http://java.sun.com/dtd/web-app_3_1.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>HolaServlet</servlet-name>
  	<display-name>HolaServlet</display-name>
  	<description></description>
  	<servlet-class>com.ricardo.HolaServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CacaServlet</servlet-name>
  	<display-name>CacaServlet</display-name>
  	<description></description>
  	<servlet-class>com.ricardo.CacaServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>HolaServlet</servlet-name>
  	<url-pattern>/hola</url-pattern>
  </servlet-mapping>

  	<!-- hola es la ruta de acceso para HolaServlet-->
</web-app>
