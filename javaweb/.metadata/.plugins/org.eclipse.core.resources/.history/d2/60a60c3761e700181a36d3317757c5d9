package com.ricardo.modelo;

public class Cancion {

	 private String titulo;
	 private String autor;
	 private String imagen;
	 private int id;
	 
	public Cancion(String titulo, String autor, String imagen, int id) {
		this.titulo = titulo;
		this.autor = autor;
		this.imagen = imagen;
		this.id = id;
	}
	 
	 
	 
	 
	 
	 
}

