package com.ricardo.modelos;
//modelo de MVC:
public class Usuario {

	public int id;
	public String name;
	public String email;
	
	public Usuario(int id, String name, String email) {
		this.id = id;
		this.name = name;
		this.email = email;
	}

	private int getId() {
		return id;
	}

	private void setId(int id) {
		this.id = id;
	}

	private String getName() {
		return name;
	}

	private void setName(String name) {
		this.name = name;
	}

	private String getEmail() {
		return email;
	}

	private void setEmail(String email) {
		this.email = email;
	}
	
	
	
	
	
}
